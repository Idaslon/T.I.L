{
  "name": "youtube-clone-backend",
  "version": "1.0.0",
  "main": "src/server.ts",
  "author": "DestroyeerU <destroyeeru18@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions .js,.ts --out-dir dist --copy-files --no-copy-ignored",
    "start": "node ./dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --no-notify --ignore-watch node_modules src/server.ts",
    "migrate": "prisma format && dotenv -e .env -- prisma migrate save -c --experimental && dotenv -e .env -- prisma migrate up --experimental && dotenv -e .env -- prisma generate",
    "migrate:reset": "dotenv -e .env -- prisma migrate down `20200610103043-create-users` --experimental",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@prisma/client": "^2.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "lodash.template": "^4.5.0",
    "multer": "^1.4.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@prisma/cli": "^2.3.0",
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/multer": "^1.4.3",
    "@types/socket.io": "^2.1.10",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "dotenv-cli": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
